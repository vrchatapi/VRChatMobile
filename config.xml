<?xml version='1.0' encoding='utf-8'?>
<widget id="com.roxyboxxy.vrchatmobile" version="1.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VRChatMobile</name>
    <description>
        A simple tool for managing your friends and account for VRChat.
    </description>
    <author email="dev@cordova.apache.org" href="https://vrchap.net/mobile/">
        The VRChap Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <icon density="xhdpi" height="57" platform="android" src="res/icon/android/icon-96-xhdpi.png" width="57" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-firebase" spec="^1.0.5" />
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <engine name="android" spec="^7.1.4" />
    <engine name="windows" spec="^5.0.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
